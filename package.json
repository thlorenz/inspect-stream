{
  "name": "inspect-stream",
  "version": "0.1.0",
  "description": "Inspects a nodejs stream 2+  and logs the data that's coming through.",
  "main": "inspect-stream.js",
  "browser": {
    "stream": "readable-stream"
  },
  "scripts": {
    "test": "set -e; for t in test/*.js; do node $t; done"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/thlorenz/inspect-stream.git"
  },
  "homepage": "https://github.com/thlorenz/inspect-stream",
  "dependencies": {
    "readable-stream": "~1.1.9"
  },
  "devDependencies": {
    "tape": "~1.0.4",
    "stream-spectrum": "~0.1.1"
  },
  "keywords": [
    "tap",
    "inspect",
    "log",
    "stream"
  ],
  "author": {
    "name": "Thorsten Lorenz",
    "email": "thlorenz@gmx.de",
    "url": "http://thlorenz.com"
  },
  "license": {
    "type": "MIT",
    "url": "https://github.com/thlorenz/inspect-stream/blob/master/LICENSE"
  },
  "engine": {
    "node": ">=0.6"
  },
  "testling": {
    "files": "test/*.js",
    "browsers": [
      "ie/8..latest",
      "firefox/17..latest",
      "firefox/nightly",
      "chrome/22..latest",
      "chrome/canary",
      "opera/12..latest",
      "opera/next",
      "safari/5.1..latest",
      "ipad/6.0..latest",
      "iphone/6.0..latest",
      "android-browser/4.2..latest"
    ]
  }
}
